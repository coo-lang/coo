def sort(array: []int, n: int, cmp: (int, int)->bool) : void {
    for var i = 0; i < n; i = i + 1 {
        for var j = 0; j < n - 1 - i; j = j + 1 {
            if cmp(array[j], array[j + 1]) == false {
                var temp = array[j]
                array[j] = array[j + 1]
                array[j + 1] = temp
            }
        }
    }
}

var arr: [10]int = {8, 2, 1, 5, 6, 3, 4, 0, 9, 7}
sort(arr, 10, (a: int, b: int): bool-> {
    if a <= b {
        ret true
    } else {
        ret false
    }
})

